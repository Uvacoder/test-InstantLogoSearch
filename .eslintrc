{
	"extends": "xo",
	"rules": {
		"camelcase": 0,
		"key-spacing": [2, {
			"align": "value",
			"beforeColon": false,
			"afterColon": true
		}],
		"new-cap": 0,
		"no-mixed-spaces-and-tabs": [2, "smart-tabs"],
		"no-multi-spaces": 0,
		"object-curly-spacing": [2, "always"],
		"space-before-function-paren": [2, "never"]
	}
}
